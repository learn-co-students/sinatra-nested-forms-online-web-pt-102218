<h1>Pirate</h1>
  <div class="pirate">
    <h3>Name: <%=@pirate.name%></h3><br>
    <h4>Weight: <%=@pirate.weight%></h4><br>
    <h5>Height: <%=@pirate.height%></h5>
    </div><br>


<h2>Ships</h2>
<% @ships.each do |ship|%>
  <div class="ship">
    <p>Ship name: <%=ship.name%></p>
    <p>Ship type: <%=ship.type%></p>
    <p>Ship booty: <%=ship.booty%></p>
  </div><br>
  <% end %>


